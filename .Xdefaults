! use xrdb -merge ~/.config/awesome/.Xdefaults
! rofi
rofi.lines: 5
rofi.eh: 1
rofi.width: 100
rofi.padding: 300
rofi.font: System San Francisco Display 18

rofi.color-enabled: true
rofi.color-window: argb:FF000000, argb:0002b36, argb:0002b36
rofi.color-normal: argb:0002b36, #268bd2, argb:0002b36, argb:0002b36, #ffffff
rofi.color-active: argb:0002b36, #268bd2, argb:0002b36, argb:0002b36, #ffffff
rofi.color-urgent: argb:0002b36, #ffffff, argb:0002b36, argb:0002b36, #fdf6e3
